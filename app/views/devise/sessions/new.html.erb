
<div class="container">

<div class="center"><%= image_tag "4.png" %></div>
<h2 class="center-align">No One Deserves To Be Left Alone</h2>
<div class="center"><%= image_tag "ty.jpg" %></div>
<p class="flow-text">UPL&hearts;FT is a safe online community for LGBTQ youth to find support and resources while on thier journey of self-discovery.</p>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.label :email, style: 'color: #bdc3c7;'%><br />
    <%= f.email_field :email, autofocus: true %>
  </div>

  <div class="field">
    <%= f.label :password, style: 'color: #bdc3c7;' %><br />
    <%= f.password_field :password, autocomplete: "off" %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me, id: 'test5' %>
      <%= f.label :remember_me, style: 'color: #bdc3c7;' %>
    </div>
  <% end -%>

  <button class="btn waves-effect waves-light" type="login" name="action">
    <%= f.submit "Log in" %>
  </button>
<% end %>

<%= render "devise/shared/links" %>
</div>
